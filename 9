#include <iostream>
using namespace std;

int main() {
    int a,b,c;
    cout<<"enter first value : ";
    cin>>a;
    cout<<"enter second value : ";
    cin>>b;
    cout<<"enter third value : ";
    cin>>c;
    int *ptrA=&a;
    int *ptrB=&b;
    int *ptrC=&c;
    int min=0;
    if (*ptrA<*ptrB && *ptrA<*ptrC ) {
       min=*ptrA;
    }
    else if (*ptrA>*ptrB && *ptrB<*ptrC )
       min=*ptrB;
    else {
        min=*ptrC;
    }

    cout<<" min is: "<< min<<endl;



    return 0;
}
